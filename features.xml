<index>
  <concepts>
    <!-- 
      The concepts are used when a blockly page is loaded it determines what is loaded into the toolbox
    -->
    <concept name='NL SQL'/>
    <concept name='Charts'/>
  </concepts>
  <feature>
    <!-- Not associated with any concept so always present -->
    <name>code blocks</name>
    <definition>codeBlocks/baseBlock.xml</definition>
  </feature>
  <feature>
    <!-- There may be 1 or more associated concepts -->
    <concept name='NL SQL'/>
    <name>NL SQL</name>
    <definition>dataSources/sql/nlSql.xml</definition>
  </feature>
  <feature>
    <concept name='Charts'/>
    <name>jqPlot interface</name>
    <definition>output/dataPlot/blocks_to_jqPlot.xml</definition>
  </feature>
</index>
